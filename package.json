    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron-store": "^6.0.0"
  },
  "devDependencies": {
    "electron-builder": "^26.4.0",
    "electron": "^40.0.0"
  },
  "build": {
    "appId": "com.electron.annuairetelephonique",
    "productName": "Annuaire Téléphonique",
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "choice.html",
      "style.css",
      "choice.css",
      "index.js",
      "choice.js",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "build/python",
        "to": "build/python",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
