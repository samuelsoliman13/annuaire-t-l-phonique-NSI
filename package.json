  "homepage": "https://github.com/samuelsoliman13/annuaire-t-l-phonique-NSI#readme",
  "bugs": {
    "url": "https://github.com/samuelsoliman13/annuaire-t-l-phonique-NSI/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelsoliman13/annuaire-t-l-phonique-NSI.git"
